{
  "name": "voting-server",
  "version": "0.0.1",
  "description": "In an effort to always be learning, I am seeking to improve my skills with JavaScript (including ES6), React, and Redux. I am also trying to learn how to test my JavaScript, a skill set I am sorely lacking in.",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "start": "babel-node index.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewtpoe/voting-server.git"
  },
  "keywords": [
    "learn",
    "react",
    "redux",
    "es6",
    "mocha",
    "chai",
    "immutable"
  ],
  "author": "Andrew T. Poe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewtpoe/voting-server/issues"
  },
  "homepage": "https://github.com/andrewtpoe/voting-server#readme",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "mocha": "^2.3.4"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "redux": "^3.0.5",
    "socket.io": "^1.3.7"
  }
}
